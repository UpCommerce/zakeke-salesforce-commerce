<isif condition="${lineItem.appliedPromotions}">
    <isloop items="${lineItem.appliedPromotions}" var="appliedPromotion">
    	<isif condition="${!(appliedPromotion.promotionID===lineItem.ZAKEKE_CompositionID)}">
        	<div>${appliedPromotion.callOutMsg}</div>
        </isif>
    </isloop>
</isif>
